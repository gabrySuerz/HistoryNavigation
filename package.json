{
    "name": "history-navigation",
    "displayName": "History Navigation",
    "description": "Navigate through the list of the recent editors",
    "version": "1.0.0",
    "publisher": "gabry",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.goToNextPoint",
                "title": "Next",
                "icon":{
                    "light": "./media/lnr-arrow-right.svg",
                    "dark": "./media/lnr-arrow-right-white.svg"
                }
            },
            {
                "command": "extension.openEditorHistory",
                "title": "History of Previous Editors",
                "icon":{
                    "light": "./media/lnr-list.svg",
                    "dark": "./media/lnr-list-white.svg"
                }
            },
            {
                "command": "extension.goToPreviousPoint",
                "title": "a",
                "icon":{
                    "light": "./media/lnr-arrow-left.svg",
                    "dark": "./media/lnr-arrow-left-white.svg"
                }
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "when": "editorFocus",
                    "command": "extension.goToNextPoint",
                    "group": "navigation"
                },
                {
                    "when": "editorFocus",
                    "command": "extension.openEditorHistory",
                    "group": "navigation"
                },
                {
                    "when": "editorFocus",
                    "command": "extension.goToPreviousPoint",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}