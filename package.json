{
  "name": "history-navigation",
  "displayName": "History Navigation",
  "description": "Navigate through the list of the recent editors",
  "version": "1.0.0",
  "publisher": "gabry",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "historynavigation.goToNextChange",
        "title": "5 - Next Edit Location",
        "icon": {
          "light": "./media/lnr-arrow-right.svg",
          "dark": "./media/lnr-arrow-right-white.svg"
        }
      },
      {
        "command": "historynavigation.goToNextEditor",
        "title": "4 - Next Editor",
        "icon": {
          "light": "./media/lnr-arrow-right.svg",
          "dark": "./media/lnr-arrow-right-white.svg"
        }
      },
      {
        "command": "historynavigation.openEditorHistory",
        "title": "3 - History of Previous Editors",
        "icon": {
          "light": "./media/lnr-list.svg",
          "dark": "./media/lnr-list-white.svg"
        }
      },
      {
        "command": "historynavigation.goToPreviousEditor",
        "title": "2 - Previous editor",
        "icon": {
          "light": "./media/lnr-arrow-left.svg",
          "dark": "./media/lnr-arrow-left-white.svg"
        }
      },
      {
        "command": "historynavigation.goToPreviousChange",
        "title": "1 - Previous Edit Location",
        "icon": {
          "light": "./media/lnr-arrow-left.svg",
          "dark": "./media/lnr-arrow-left-white.svg"
        }
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "textInputFocus",
          "command": "historynavigation.goToNextChange",
          "group": "navigation"
        },
        {
          "when": "textInputFocus",
          "command": "historynavigation.goToNextEditor",
          "group": "navigation"
        },
        {
          "when": "textInputFocus",
          "command": "historynavigation.openEditorHistory",
          "group": "navigation"
        },
        {
          "when": "textInputFocus",
          "command": "historynavigation.goToPreviousEditor",
          "group": "navigation"
        },
        {
          "when": "textInputFocus",
          "command": "historynavigation.goToPreviousChange",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^8.10.25",
    "tslint": "^5.11.0",
    "typescript": "^3.5.3",
    "vscode": "^1.1.34"
  },
  "dependencies": {}
}
